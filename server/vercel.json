{
    "version" : 2,
    "builds" : [
        {
            "src": "*.js",
            "use": "@vercel/node"
        }
    ],
    "routes" : [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "headers": [
      {
        "source": "https://baylerview-server.vercel.app/api/contacts", 
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Methods",
            "value": "GET, POST, PUT, DELETE"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "Content-Type"
          }
        ]
      }
    ]
  }
  